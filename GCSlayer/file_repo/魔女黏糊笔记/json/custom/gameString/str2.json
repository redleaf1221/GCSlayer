    "isCustomPhraseDesc": true,
    "customPhraseDescTS": "// 根据属性名称值编写自定义短语描述，如：\"我的短语是\"+attrs.xxx\n// 使用下拉框组件的变量attrs.xxx有对应的attrs.xxxItems的属性以便获得标签名集合\n// 获取自定义模块的数据名称的函数：getModuleDataName(moduleID:number,dataID:number):string;\n// 获取各种列表内数据名称的函数：getTypeDataListName(typeID: number, dataID: number):string;\n// -- typeID ：0-全局变量 1-全局开关 2-全局字符串 3-玩家变量 4-玩家开关 5-玩家字符串\n//           ：6-场景列表 7-场景对象模型列表 8-图块列表 9-行走图列表 10-行走图动作列表 11-立绘列表 12-立绘表情列表\n//           ：13-公共事件 14-对话框列表 15-动画列表 16-动画信号列表 17-界面列表 18-数据结构列表 19-自定义模块列表\n//           ：20-自定义命令列表 21-自定义行为列表 22-自动元件列表 23-行走图部件列表 24-立绘部件列表\nvar str = \"\";\nstr += attrs.soTypeItems[attrs.soType];\nif (attrs.soType == 3) {\n    str += \":\";\n    if (attrs.useVar) {\n        str += ` [${attrs.varID}-${getTypeDataListName(3, attrs.varID)}]`;\n    }\n    else {\n        str += ` ${attrs.no}号`;\n    }\n}\nstr += `的`;\nif (attrs.type == 1) {\n    let mapping = { 0: 3, 1: 5, 2: 4 };\n    if (attrs.customAttr.selectMode == 1) {\n        let mode = attrs.customAttr.inputModeInfo.mode;\n        let constName = attrs.customAttr.inputModeInfo.constName;\n        let varNameIndex = attrs.customAttr.inputModeInfo.varNameIndex;\n        let varName = mode == 0 ? constName : `[@${varNameIndex}-${getTypeDataListName(mapping[varNameIndex], varNameIndex)}]`;\n        str += `「${varName}」`;\n    }\n    else {\n        str += `「${attrs.customAttr.varName}」`;\n    }\n}\nelse if (attrs.type == 2) {\n    let mapping = { 0: 3, 1: 5, 2: 4 };\n    if (attrs.soModuleAttr.selectMode == 1) {\n        let mode = attrs.soModuleAttr.inputModeInfo.mode;\n        let constName = attrs.soModuleAttr.inputModeInfo.constName;\n        let varNameIndex = attrs.soModuleAttr.inputModeInfo.varNameIndex;\n        let varName = mode == 0 ? constName : `[@${varNameIndex}-${getTypeDataListName(mapping[varNameIndex], varNameIndex)}]`;\n        str += ` 模块「${attrs.soModuleAttr.moduleID}-${getTypeDataListName(26, attrs.soModuleAttr.moduleID)}」.${varName}`;\n    }\n    else {\n        str += ` 模块「${attrs.soModuleAttr.moduleID}-${getTypeDataListName(26, attrs.soModuleAttr.moduleID)}」.${attrs.soModuleAttr.varName}`;\n    }\n}\nelse {\n    str += `「${attrs.typeItems[attrs.type]}」`;\n}",
    "customPhraseDescJS": "var str = \"\";\nstr += attrs.soTypeItems[attrs.soType];\nif (attrs.soType == 3) {\n    str += \":\";\n    if (attrs.useVar) {\n        str += \" [\" + attrs.varID + \"-\" + getTypeDataListName(3, attrs.varID) + \"]\";\n    }\n    else {\n        str += \" \" + attrs.no + \"\\u53F7\";\n    }\n}\nstr += \"\\u7684\";\nif (attrs.type == 1) {\n    var mapping = { 0: 3, 1: 5, 2: 4 };\n    if (attrs.customAttr.selectMode == 1) {\n        var mode = attrs.customAttr.inputModeInfo.mode;\n        var constName = attrs.customAttr.inputModeInfo.constName;\n        var varNameIndex = attrs.customAttr.inputModeInfo.varNameIndex;\n        var varName = mode == 0 ? constName : \"[@\" + varNameIndex + \"-\" + getTypeDataListName(mapping[varNameIndex], varNameIndex) + \"]\";\n        str += \"\\u300C\" + varName + \"\\u300D\";\n    }\n    else {\n        str += \"\\u300C\" + attrs.customAttr.varName + \"\\u300D\";\n    }\n}\nelse if (attrs.type == 2) {\n    var mapping = { 0: 3, 1: 5, 2: 4 };\n    if (attrs.soModuleAttr.selectMode == 1) {\n        var mode = attrs.soModuleAttr.inputModeInfo.mode;\n        var constName = attrs.soModuleAttr.inputModeInfo.constName;\n        var varNameIndex = attrs.soModuleAttr.inputModeInfo.varNameIndex;\n        var varName = mode == 0 ? constName : \"[@\" + varNameIndex + \"-\" + getTypeDataListName(mapping[varNameIndex], varNameIndex) + \"]\";\n        str += \" \\u6A21\\u5757\\u300C\" + attrs.soModuleAttr.moduleID + \"-\" + getTypeDataListName(26, attrs.soModuleAttr.moduleID) + \"\\u300D.\" + varName;\n    }\n    else {\n        str += \" \\u6A21\\u5757\\u300C\" + attrs.soModuleAttr.moduleID + \"-\" + getTypeDataListName(26, attrs.soModuleAttr.moduleID) + \"\\u300D.\" + attrs.soModuleAttr.varName;\n    }\n}\nelse {\n    str += \"\\u300C\" + attrs.typeItems[attrs.type] + \"\\u300D\";\n}\n"
}